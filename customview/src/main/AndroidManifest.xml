<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jimmy.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.CustomViewActivity1" />
        <activity android:name=".ui.CustomViewActivity2" />
        <activity android:name=".ui.CustomViewActivity3" />
        <activity android:name=".ui.CustomViewActivity5" />
        <activity android:name=".ui.CustomViewActivity6" />
        <activity android:name=".ui.CustomViewActivity4" />
    </application>

</manifest>